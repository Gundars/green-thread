{
    "name": "amphp/green-thread",
    "homepage": "http://amphp.org/amp",
    "description": "Green thread implementation based on the PHP fiber extension",
    "keywords": [
        "async",
        "asynchronous",
        "concurrency",
        "green thread",
        "thread",
        "non-blocking"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Aaron Piotrowski",
            "email": "aaron@trowski.com"
        }
    ],
    "require": {
        "ext-fiber": "*",
        "amphp/amp": "^2"
    },
    "require-dev": {
        "amphp/phpunit-util": "^1",
        "react/promise": "^2",
        "friendsofphp/php-cs-fixer": "^2.3",
        "phpunit/phpunit": "^6.0.9"
    },
    "autoload": {
        "psr-4": {
            "Amp\\GreenThread\\": "src"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Amp\\GreenThread\\Test\\": "test"
        }
    },
    "support": {
        "issues": "https://github.com/amphp/green-thread/issues",
        "irc": "irc://irc.freenode.org/amphp"
    },
    "config": {
        "platform": {
            "php": "7.0.0"
        }
    }
}
